<GroupBox Name="GBDEDITsubinventory"><Ctrls>
<GroupBox Name="GB_subinventory" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_subinventory" Size="60,20" Text="subinventory" Align="Right"/>
<TextBox Name="subinventory" Size="170,20" Text="" Weight="170" ToolTip="ttp_subinventory"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_type" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_type" Size="60,20" Text="type" Align="Right" onload="init"/>
<ComboBox Name="type" Size="170,100" Text="" Weight="170"  ToolTip="ttp_type" onload="init">
	<DbDetails>
<DbTable>xbs.option_line</DbTable>
<Index>option_line_id</Index>
<Data>option_line_value,option_line_id</Data>
<Where>option_header_id=88</Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_allow_negative_balance_cb" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<CheckBox Name="allow_negative_balance_cb" Size="230,20" Text="allow_negative_balance_cb" ToolTip="ttp_allow_negative_balance_cb" checked="true"></CheckBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_locator_control" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_locator_control" Size="60,20" Text="locator_control" Align="Right" onload="init"/>
<ComboBox Name="locator_control" Size="170,100" Text="" Weight="170"  ToolTip="ttp_locator_control" onload="init">
	<DbDetails>
<DbTable>xbs.option_line</DbTable>
<Index>option_line_id</Index>
<Data>option_line_value,option_line_id</Data>
<Where>option_header_id=89</Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_description" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_description" Size="60,20" Text="description" Align="Right"/>
<TextBox Name="description" Size="170,20" Text="" Weight="170" ToolTip="ttp_description"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_status" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<CheckBox Name="status" Size="230,20" Text="status" ToolTip="ttp_status"></CheckBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_created_by" Multiline="0" cols="xs12 sm6 md4" Visible="false" overlap="pre"><Ctrls>
<Label Name="lb_created_by" Size="60,20" Text="created_by" Align="Right" Visible="false" overlap="pre"/>
<TextBox Name="created_by" Size="170,20" Text="" Weight="170" ToolTip="ttp_created_by" Visible="false" overlap="pre"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_creation_date" Multiline="0" cols="xs12 sm6 md4" Visible="false" overlap="pre"><Ctrls>
<Label Name="lb_creation_date" Size="60,20" Text="creation_date" Align="Right" Visible="false" overlap="pre"/>
<DateTime Name="creation_date" Format="yyyy-MM-dd HH:mm:ss" Size="170,20" Text="" Weight="170" ToolTip="ttp_creation_date" Visible="false" overlap="pre"></DateTime>
</Ctrls></GroupBox>
<GroupBox Name="GB_last_update_by" Multiline="0" cols="xs12 sm6 md4" Visible="false" overlap="pre"><Ctrls>
<Label Name="lb_last_update_by" Size="60,20" Text="last_update_by" Align="Right" Visible="false" overlap="pre"/>
<TextBox Name="last_update_by" Size="170,20" Text="" Weight="170" ToolTip="ttp_last_update_by" Visible="false" overlap="pre"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_last_update_date" Multiline="0" cols="xs12 sm6 md4" Visible="false" overlap="pre"><Ctrls>
<Label Name="lb_last_update_date" Size="60,20" Text="last_update_date" Align="Right" Visible="false" overlap="pre"/>
<DateTime Name="last_update_date" Format="yyyy-MM-dd HH:mm:ss" Size="170,20" Text="" Weight="170" ToolTip="ttp_last_update_date" Visible="false" overlap="pre"></DateTime>
</Ctrls></GroupBox>
<GroupBox Name="GB_subinventory_id" Multiline="0" cols="xs12 sm6 md4" Visible="false" overlap="pre"><Ctrls>
<Label Name="lb_subinventory_id" Size="60,20" Text="subinventory_id" Align="Right" Visible="false" overlap="pre" readonly="true"/>
<TextBox Name="subinventory_id" Size="170,20" Text="" Weight="170" ToolTip="ttp_subinventory_id" Visible="false" overlap="pre" readonly="true"><OnEnChange><assign name="GBsubinventory" expr="TABEDITsubinventory"/></OnEnChange></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_org_id" Multiline="0" cols="xs12 sm6 md4" Visible="false" overlap="pre"><Ctrls>
<Label Name="lb_org_id" Size="60,20" Text="org_id" Align="Right" Visible="false" overlap="pre"/>
<TextBox Name="org_id" Size="170,20" Text="" Weight="170" ToolTip="ttp_org_id" Visible="false" overlap="pre"></TextBox>
</Ctrls></GroupBox>
</Ctrls></GroupBox>
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>xbs.bom_standard_operation</DbTable>
<Where>bom_standard_operation_id='%{bom_standard_operationLst[bom_standard_operation_id]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>

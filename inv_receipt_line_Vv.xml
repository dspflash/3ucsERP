<GroupBox Name="GBDVinv_receipt_line" cols="xs12 sm12 md12 lg12"><Ctrls>
<ListCtrl Name="inv_receipt_line_vLst" ToolTip="ttp_inv_receipt_line_v" Size="700,450" Heading="reference_key_value,120;transaction_quantity,75;item_id_m,120;uom_id,120;subinventory_id,120;locator_id,120;lot_number,120;currency,120;doc_currency,55;created_by,60;creation_date,150;last_update_by,60;last_update_date,150;inv_receipt_line_id,60;status,30;inspection_status,30;inspection_quality,30;" loadedclick="0">
	<DbDetails >
<Index>inv_receipt_line_id</Index>
<DbTable>xbs.inv_receipt_line i left join xbs.item i0 on i.item_id_m=i0.item_id_m left join xbs.uom u on i.uom_id=u.uom_id left join xbs.subinventory s on i.subinventory_id=s.subinventory_id left join xbs.locator l on i.locator_id=l.locator_id left join xbs.inv_lot_number i1 on i.lot_number=i1.inv_lot_number_id left join xbs.currency c on i.currency=c.currency_id</DbTable>
<Data>i.reference_key_value,i.transaction_quantity,i0.item_number item_id_m,u.uom_name uom_id,s.subinventory subinventory_id,l.locator locator_id,i1.lot_number lot_number,c.title currency,i.doc_currency,i.created_by,i.creation_date,i.last_update_by,i.last_update_date,i.inv_receipt_line_id,i.status,i.inspection_status,i.inspection_quality</Data>
<Order>inv_receipt_line_id</Order></DbDetails>
<OnClick><assign name="reference_key_value[refid]" expr="inv_receipt_lineLst[reference_key_value]" function="setsel"/></OnClick>
<operator refresh="1" initial="0" firstpage="0" prepage="0" nextpage="0">
<op text="addone" mustselected="0"><if cond="!%{inv_receipt_header_id}"><alert expr="inv_receipt_header_id is Empty!"/><else/><submit next="$parent.inv_receipt_line.GBDEDITinv_receipt_line" function="reset"/><assign name="$parent.inv_receipt_line.inv_receipt_header_id" expr="%{inv_receipt_header_id}"/><assign name="$parent.inv_receipt_line.frmwndctrl" expr="%{this}"/></if></op>
<op text="modify"><DbDetails><Where>inv_receipt_line_id='%{inv_receipt_line_vLst[inv_receipt_line_id]}'</Where></DbDetails><submit namelist="DbDetails" next="$parent.inv_receipt_line.inv_receipt_lineLst"/><assign name="$parent.inv_receipt_line.frmwndctrl" expr="%{this}"/></op>
<op text="delete" needconfirm="1"><DbDetails>
<DbTable>xbs.inv_receipt_line</DbTable>
<Where>inv_receipt_line_id='%{inv_receipt_line_vLst[inv_receipt_line_id]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>
<submit namelist="DbDetails"/><submit/></op>
</operator>
</ListCtrl>
</Ctrls></GroupBox>
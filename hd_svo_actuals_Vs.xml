<TextBox Name="hd_svo_actuals_id" Size="170,20" Text="" Weight="170" ToolTip="ttp_hd_svo_actuals_id" Visible="false" overlap="pre" readonly="true"><OnEnChange><if cond="'%{hd_svo_actuals_id}'"><SetVisible name="inserthd_svo_actuals" Visible="0"/><SetVisible name="modifyhd_svo_actuals" Visible="1"/><else/><SetVisible name="inserthd_svo_actuals" Visible="1"/><SetVisible name="modifyhd_svo_actuals" Visible="0"/></if></OnEnChange></TextBox>

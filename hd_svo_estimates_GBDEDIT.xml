<TextBox Weight="170" Text="" ToolTip="ttp_action_type_id" Name="action_type_id" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_price_date"><Ctrls>

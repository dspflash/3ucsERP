	<submit next="GBDEDIThd_service_contract_header" function="reset"/>
	</OnClick>
	</Button><TAB/>
<Button Text="gsearch" ToolTip="ttp_gsearchhd_service_contract_header" Name="gsearchhd_service_contract_header" Size="50,20" visible="false">

<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_contactid" Visible="false"><Ctrls>
<Label Align="Right" Text="contactid" overlap="pre" Name="lb_contactid" readonly="true" Size="60,20" Visible="false"/>
<TextBox Weight="170" Text="" ToolTip="ttp_contactid" overlap="pre" Name="contactid" readonly="true" Size="170,20" Visible="false"><OnEnChange><if cond="'%{contactid}'"><SetVisible name="insertcontact" Visible="0"/><SetVisible name="modifycontact" Visible="1"/><else/><SetVisible name="insertcontact" Visible="1"/><SetVisible name="modifycontact" Visible="0"/></if></OnEnChange></TextBox>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_reftbltp" Visible="false"><Ctrls>
<Label Align="Right" Text="reftbltp" overlap="pre" Name="lb_reftbltp" Size="60,20" Visible="false"/>
<ComboBox Weight="170" Text="" ToolTip="ttp_reftbltp" overlap="pre" Items="customer|customersite|supplier|suppliersite|org|employee|bank|banksite|bankaccount" Name="reftbltp" Size="170,100" Visible="false"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_refid" Visible="false"><Ctrls>

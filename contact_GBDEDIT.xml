<GroupBox Name="GBDEDITcontact"><Ctrls>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_firstname"><Ctrls>
<Label Align="Right" Text="firstname" Name="lb_firstname" Size="60,20"/>
<TextBox Weight="170" Text="" ToolTip="ttp_firstname" Name="firstname" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_lastname"><Ctrls>

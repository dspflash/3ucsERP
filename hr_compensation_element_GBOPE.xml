<submit next="GBDEDIThr_compensation_element" function="reset"/>
	</OnClick>
</Button><TAB/>
<Button Text="Search" ToolTip="ttp_gsearchhr_compensation_element" Name="gsearchhr_compensation_element" Size="50,20" visible="false">

<GroupBox Name="GBDEDITinv_lot_number"><Ctrls>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_lot_number"><Ctrls>
<Label Align="Right" Text="lot_number" Name="lb_lot_number" Size="60,20"/>
<TextBox Weight="170" ToolTip="ttp_lot_number" Name="lot_number" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_quantity"><Ctrls>
<Label Align="Right" Text="quantity" Name="lb_quantity" readonly="true" Size="60,20"/>
<TextBox Weight="170" ToolTip="ttp_quantity" Name="quantity" readonly="true" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_item_id_m"><Ctrls>
<Label Align="Right" Text="item_id_m" Name="lb_item_id_m" Size="60,20" selectedIndex="0"/>
<ComboBox Weight="170" ToolTip="ttp_item_id_m" Name="item_id_m" Size="170,100" selectedIndex="0">
	<DbDetails>
<DbTable>xbs.item</DbTable>
<Index>item_id_m</Index>
<Data>item_number,item_id_m</Data>
<Where/>
<queryFormat/></DbDetails></ComboBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_item_revision"><Ctrls>
<Label Align="Right" Text="item_revision" Name="lb_item_revision" readonly="true" Size="60,20"/>
<TextBox Weight="170" ToolTip="ttp_item_revision" Name="item_revision" readonly="true" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_generation"><Ctrls>
<Label Align="Right" Text="generation" Name="lb_generation" Size="60,20"/>
<ComboBox Weight="170" ToolTip="ttp_generation" Items="PRE_DEFINED|At Receipt|At Shipment" Name="generation" Size="170,100"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_reference_key_name"><Ctrls>
<Label Align="Right" Text="reference_key_name" Name="lb_reference_key_name" readonly="true" Size="60,20"/>
<TextBox Weight="170" ToolTip="ttp_reference_key_name" Name="reference_key_name" readonly="true" Size="170,20"/>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_reference_key_value"><Ctrls>
<Label Align="Right" Text="reference_key_value" Name="lb_reference_key_value" readonly="true" Size="60,20"/>
<TextBox Weight="170" ToolTip="ttp_reference_key_value" Name="reference_key_value" readonly="true" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_origination_type"><Ctrls>
<Label Align="Right" Text="origination_type" Name="lb_origination_type" Size="60,20"/>
<ComboBox Weight="170" ToolTip="ttp_origination_type" Items="Inventory|WIP_COMPLETION|RECEVING|SHIPMENT|MANUAL|SYS_GEN" Name="origination_type" Size="170,100"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_origination_date"><Ctrls>
<Label Align="Right" Text="origination_date" Name="lb_origination_date" Size="60,20"/>
<DateTime Weight="170" ToolTip="ttp_origination_date" Name="origination_date" Size="170,20" Format="yyyy-MM-dd"/>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_country_of_origin"><Ctrls>
<Label Align="Right" Text="country_of_origin" onload="init" Name="lb_country_of_origin" Size="60,20"/>
<ComboBox Weight="170" ToolTip="ttp_country_of_origin" onload="init" Name="country_of_origin" Size="170,100">
	<DbDetails>
<DbTable>xbs.country</DbTable>
<Index>country_id</Index>
<Data>namecn,country_id</Data>
<Where>namecn is not null</Where>
<queryFormat/></DbDetails></ComboBox>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_description"><Ctrls>
<Label Align="Right" Text="description" Name="lb_description" Size="60,20"/>
<TextBox Weight="170" ToolTip="ttp_description" Name="description" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_created_by" Visible="false"><Ctrls>
<Label Align="Right" Text="created_by" overlap="pre" Name="lb_created_by" Size="60,20" Visible="false"/>
<TextBox Weight="170" ToolTip="ttp_created_by" overlap="pre" Name="created_by" Size="170,20" Visible="false"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_creation_date" Visible="false"><Ctrls>
<Label Align="Right" Text="creation_date" overlap="pre" Name="lb_creation_date" Size="60,20" Visible="false"/>
<DateTime Weight="170" ToolTip="ttp_creation_date" overlap="pre" Name="creation_date" Size="170,20" Visible="false" Format="yyyy-MM-dd HH:mm:ss"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_last_update_by" Visible="false"><Ctrls>
<Label Align="Right" Text="last_update_by" overlap="pre" Name="lb_last_update_by" Size="60,20" Visible="false"/>
<TextBox Weight="170" ToolTip="ttp_last_update_by" overlap="pre" Name="last_update_by" Size="170,20" Visible="false"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_last_update_date" Visible="false"><Ctrls>
<Label Align="Right" Text="last_update_date" overlap="pre" Name="lb_last_update_date" Size="60,20" Visible="false"/>
<DateTime Weight="170" ToolTip="ttp_last_update_date" overlap="pre" Name="last_update_date" Size="170,20" Visible="false" Format="yyyy-MM-dd HH:mm:ss"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_org_id" Visible="false"><Ctrls>
<Label Align="Right" Text="org_id" overlap="pre" Name="lb_org_id" Size="60,20" Visible="false"/>
<TextBox Weight="170" ToolTip="ttp_org_id" overlap="pre" Name="org_id" Size="170,20" Visible="false"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_inv_lot_number_id" Visible="false"><Ctrls>
<Label Align="Right" Text="inv_lot_number_id" overlap="pre" Name="lb_inv_lot_number_id" readonly="true" Size="60,20" Visible="false"/>
<TextBox Weight="170" ToolTip="ttp_inv_lot_number_id" overlap="pre" Name="inv_lot_number_id" readonly="true" Size="170,20" Visible="false"><OnEnChange><assign name="GBinv_lot_number" expr="TABEDITinv_lot_number"/></OnEnChange></TextBox>
</Ctrls></GroupBox>
</Ctrls></GroupBox>
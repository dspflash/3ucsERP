<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GBPGhr_expense_line"><Ctrls>
<Label Align="Right" Text="Records" Name="lb_Records" Size="60,20"/>
<TextBox Weight="30" ToolTip="ttp_records" Name="records_hr_expense_line" Size="30,20" OnLoad="init" readonly="1">
	<DbDetails fcsm="1">
<Index>hr_expense_line_id</Index>
<DbTable>xbs.hr_expense_line h left join xbs.currency c on h.receipt_currency=c.currency_id left join xbs.hr_expense_tpl_line h0 on h.expense_type=h0.hr_expense_tpl_line_id left join xbs.option_line o on h.status=o.option_line_id left join xbs.hr_location h1 on h.expense_location=h1.hr_location_id left join xbs.uom u on h.mileage_uom_id=u.uom_id</DbTable>
<Data>count(*) records</Data>
<Order>hr_expense_line_id</Order><Queryformat>select %{data} from (select h.hr_expense_line_id from %{dbtable} %{where} GROUP BY h.hr_expense_line_id  ORDER BY h.hr_expense_line_id) t</Queryformat></DbDetails>
<OnEnchange>
<assign name="pgs_hr_expense_line" expr="js(Math.ceil(%{records_hr_expense_line}/%{hr_expense_line_vLst[countsperpage]}))"/>
<assign name="pgscb_hr_expense_line" expr="js('1~'+Math.ceil(%{records_hr_expense_line}/%{hr_expense_line_vLst[countsperpage]}))" function="setitems"/>
</OnEnchange>
</TextBox>
<!--ComboBox Weight="60" ToolTip="ttp_pgs" Name="pgscb_hr_expense_line" Size="60,20"><OnSelEndOK><submit next="hr_expense_line_vLst" function="GetPage(%{pgscb_hr_expense_line}-1)"/></OnSelEndOK></ComboBox-->
<TextBox Weight="30" ToolTip="ttp_pgs" Name="pgs_hr_expense_line" Size="30,20" readonly="1"/>
<TextBox Weight="30" ToolTip="ttp_pgcur" Name="pgcur_hr_expense_line" Size="30,20"/>
<Button Text="go2Page" Name="go2pagehr_expense_line_vLst" Size="50,20"><OnClick><submit next="hr_expense_line_vLst" function="GetPage(%{pgcur_hr_expense_line}-1)"/></OnClick></Button>
</Ctrls></GroupBox>

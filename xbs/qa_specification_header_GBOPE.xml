<GroupBox cols="xs12 sm12 md12 lg12" Name="GBOPEqa_specification_header" Size="100,100"><Ctrls>
<Button sqlpkinsert="qa_specification_header_id" Text="create" ToolTip="ttp_createqa_specification_header" Name="insertqa_specification_header" Size="50,20">
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>xbs.qa_specification_header</DbTable>
<Required>%{dataRequired}</Required>
<Requireless>%{dataRequireless}</Requireless>
<Data>specification_name,effective_from,effective_to,item_id_m[item_id_m],item_revision,description,ap_supplier_id[supplier_id],ar_customer_id[ar_customer_id],status</Data>
<DataEx>created_by,creation_date,last_update_by,last_update_date;'%{user_id}',now(),'%{user_id}',now()</DataEx>
<queryFormat>insert into %{table} %{data}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><if cond="%{refreshAfterEdit}"><submit next="qa_specification_headerLst"/></if>
	</OnClick>
	</Button><TAB/>
<Button Text="modify" ToolTip="ttp_modifyqa_specification_header" Name="modifyqa_specification_header" Size="50,20">
			  <OnClick>
	<DbDetails fcsm="1">
<DbTable>xbs.qa_specification_header</DbTable>
<Required>%{dataRequired}</Required>
<Requireless>%{dataRequireless}</Requireless>
<Data>specification_name,effective_from,effective_to,item_id_m[item_id_m],item_revision,description,ap_supplier_id[supplier_id],ar_customer_id[ar_customer_id],status</Data>
<DataEx>last_update_by,last_update_date;'%{user_id}',now()</DataEx>
<Where>qa_specification_header_id='%{qa_specification_header_id}'</Where>
<queryFormat>update %{table} set %{data} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><if cond="%{refreshAfterEdit}"><submit next="qa_specification_headerLst"/></if>
	</OnClick>
	</Button><TAB/>
<Button Text="reset" ToolTip="ttp_resetqa_specification_header" Name="resetqa_specification_header" Size="50,20">
	<OnClick>
	<submit next="GBDEDITqa_specification_header" function="reset"/>
	</OnClick>
	</Button><TAB/>
<Button Text="gsearch" ToolTip="ttp_gsearchqa_specification_header" Name="gsearchqa_specification_header" Size="50,20" visible="false">
			  <OnClick>
	<search next="qa_specification_headerLst" listed="0" prefixwhere="0" inputed="1"><zone_id op="(" ref=""/></search><submit next="GBDEDITqa_specification_header" namelist="search" function="search"/>
	</OnClick>
	</Button><TAB/>
</Ctrls></GroupBox>
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>xbs.hd_subscription_detail</DbTable>
<Where>hd_subscription_detail_id='%{hd_subscription_detailLst[hd_subscription_detail_id]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>

	<OnClick>
	<DbDetails fcsm="1">
<DbTable>xbs.qa_cp_header</DbTable>
<Where>qa_cp_header_id='%{qa_cp_headerLst[qa_cp_header_id]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>

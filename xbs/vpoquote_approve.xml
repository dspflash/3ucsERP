	<GroupBox cols="xs12 sm12 md12 lg12" Name="GBOPDVitem"><Ctrls>
	<ListCtrl loadedclick="0" colsDisplay="23" PageSize="500" ToolTip="ttp_item" Name="PO_quoteLst" onload="init" Size="700,420">
		<DbDetails>
		<DbTable>xbs.po_quote_header_v</DbTable>
		<Index>po_quote_header_id</Index>
		<Data>*</Data>
		<Order>po_quote_header_id</Order>
		<Where>and approval_status=324</Where>
		<queryFormat>select %{data} from %{dbtable} where bu_org_id='%{orgid}' and approval_status in (323,324,326) %{where} limit %{StartRow},%{PgSize}</queryFormat>
		</DbDetails>
		<operator refresh="1" firstpage="1" prepage="1" nextpage="1">
<EvalTbl rules="" table="quote_approval_rule">
<rules><xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="rule/quote_approval_rule.xml"/></rules>
<inputs approval_status="%{PO_quoteLst[approval_status]}"/>
<outputs title="text" disabled="disabled"/></EvalTbl>
		<op text="view"><DbDetails><Where>po_quote_header_id='%{$this.po_quotelst[po_quote_header_id]}'</Where></DbDetails><submit namelist="DbDetails" next="$this.vpo_quote_line.PO_quoteLst"/></op>
		<op text="attachment"><DbDetails><Where>reftbl='po_quote_header' and refid='%{$this.po_quotelst[po_quote_header_id]}'</Where></DbDetails><submit namelist="DbDetails" next="$this.attachment_v.attachment_vLst"/></op>
		<op text="reEdit" needconfirm="1"><DbDetails><queryFormat>update po_quote_header set approval_status=321,quote_status=1486,last_update_by='%{user_id}',last_update_date=now() where po_quote_header_id='%{PO_quoteLst[po_quote_header_id]}'  and quote_status=1487</queryFormat></DbDetails><submit namelist="DbDetails"/><submit/></op>
		<op text="approve" needconfirm="1"><DbDetails><queryFormat>update po_quote_header set approval_status=326,quote_status=1489,last_update_by='%{user_id}',last_update_date=now() where po_quote_header_id='%{PO_quoteLst[po_quote_header_id]}' and quote_status=1487 and approval_status=324</queryFormat></DbDetails><submit namelist="DbDetails"/><submit/></op>
		</operator>

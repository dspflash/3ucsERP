<GroupBox Name="GBDVsd_delivery_line" cols="xs12 sm12 md12 lg12"><Ctrls>
<ListCtrl Name="sd_delivery_line_vLst" ToolTip="ttp_sd_delivery_line" Size="700,180" Heading="line_number,120;so_number,120;document_extn_ref,120;item_number,120;item_description,125;item_name,125;item_specification,125;line_uom,120;quantity,100;shipped_quantity,75;staging_subinventory_id,120;staging_locator_id,120;pkg_uom,120;pkg_quantity,55;pkg_description,250;picking_date,90;actual_ship_date,90;volume_uom,120;total_volume,100;weight_uom,120;total_weight,100;created_by,60;creation_date,150;last_update_by,60;last_update_date,150;sd_delivery_line_id,60;shipping_org_id,60;" loadedclick="0">
	<DbDetails >
<DbTable>xbs.sd_delivery_line s left join xbs.sd_so_header s0 on s.sd_so_header_id=s0.sd_so_header_id left join xbs.sd_so_line s1 on s.sd_so_line_id=s1.sd_so_line_id left join xbs.item i on s.item_id_m=i.item_id_m left join xbs.uom u on s.line_uom_id=u.uom_id left join xbs.subinventory s2 on s.staging_subinventory_id=s2.subinventory_id left join xbs.locator l on s.staging_locator_id=l.locator_id left join xbs.uom u0 on s.pkg_uom_id=u0.uom_id left join xbs.uom u1 on s.volume_uom_id=u1.uom_id left join xbs.uom u2 on s.weight_uom_id=u2.uom_id</DbTable>
<Index>sd_delivery_line_id</Index>
<Data>s1.line_number,s0.so_number,s0.document_extn_ref,i.item_number,s.item_description,`i`.`item_name`,`i`.`item_specification`,u.uom_name line_uom,s.quantity,s.shipped_quantity,s2.subinventory staging_subinventory_id,l.locator staging_locator_id,u0.uom_name pkg_uom,s.pkg_quantity,s.pkg_description,s.picking_date,s.actual_ship_date,s.picking_date,s.actual_ship_date,u1.uom_name volume_uom,s.total_volume,u2.uom_name weight_uom,s.total_weight,s.created_by,s.creation_date,s.last_update_by,s.last_update_date,s.item_id_m,s.sd_delivery_line_id,s.shipping_org_id</Data>
<Order>sd_delivery_line_id</Order></DbDetails>

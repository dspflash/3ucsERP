	<DbDetails>
<DbTable>xbs.prj_rate_schedule_header</DbTable>
<Index>prj_rate_schedule_header_id</Index>
<Data>schedule_name,rate_type,prj_rate_schedule_header_id</Data>
<Where/>
<queryFormat/></DbDetails><OnSelEndOk> <if cond="%{prj_rate_schedule_header_id[rate_type]}=='Employee'"><DbDetails><DbTable>xbs.employees_v</DbTable><Data>employee_name as name,hr_employee_id refid</Data></DbDetails><submit next="reference_key_value" namelist="DbDetails"/> <elseif cond="%{prj_rate_schedule_header_id[rate_type]}=='JOB'"/><DbDetails><DbTable>xbs.hr_job</DbTable><Data>job_name as name,hr_job_id refid</Data></DbDetails><submit next="reference_key_value" namelist="DbDetails"/> </if></OnSelEndOk></ComboBox>

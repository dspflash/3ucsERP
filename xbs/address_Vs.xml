<GroupBox Name="GBDEDITaddress" text="Search" toggle="1"><Ctrls>
<GroupBox Name="GB_usage_type" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_usage_type" Size="60,20" Text="usage_type" Align="Right" onload="init" selectedIndex="3"/>
<ComboBox Name="usage_type" Size="170,100" Text="" Weight="170"  ToolTip="ttp_usage_type" onload="init" selectedIndex="3">
	<DbDetails>
<Index>option_line_id</Index>
<DbTable>xbs.option_line</DbTable>
<Data>option_line_value,option_line_id</Data>
<Where>option_header_id=230</Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_country" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_country" Size="60,20" Text="country" Align="Right" onload="init"/>
<ComboBox Name="country" Size="170,100" Text="" Weight="170"  ToolTip="ttp_country" onload="init">
	<DbDetails>
<Index>country_id</Index>
<DbTable>xbs.country</DbTable>
<Data>namecn,country_id</Data>
<Where>namecn is not null</Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_phone" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_phone" Size="60,20" Text="phone" Align="Right"/>
<TextBox Name="phone" Size="150,20" Text="" Weight="150" ToolTip="ttp_phone"></TextBox>
<MagicCtrl Name="dialphone" button="type:0" images=" ,data/startcall_green.png" Size="20,20" OnClick="nis3ucs:%{phone}" ToolTip="DialToCall"/>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_email" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_email" Size="60,20" Text="email" Align="Right"/>
<TextBox Name="email" Size="170,20" Text="" Weight="170" ToolTip="ttp_email"></TextBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_address" Multiline="0" cols="xs12 sm12 md6 lg4"><Ctrls>
<Label Name="lb_address" Size="60,50" Text="address" Align="Right"/>
<TextBox Name="address" Size="630,50" Text="" Weight="630" ToolTip="ttp_address"></TextBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_postal_code" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_postal_code" Size="60,20" Text="postal_code" Align="Right"/>
<TextBox Name="postal_code" Size="170,20" Text="" Weight="170" ToolTip="ttp_postal_code"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_default_cb" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<CheckBox Name="default_cb" Size="230,20" Text="default_cb" ToolTip="ttp_default_cb"></CheckBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_status" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<CheckBox Name="status" Size="230,20" Text="status" ToolTip="ttp_status" checked="true"></CheckBox>
</Ctrls></GroupBox>
</Ctrls></GroupBox>
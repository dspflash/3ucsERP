	<submit namelist="DbDetails"/><if cond="%{refreshAfterEdit}"><submit next="fp_mrp_planned_sourcesLst"/></if>
	</OnClick>
	</Button><TAB/>
<Button Text="reset" ToolTip="ttp_resetfp_mrp_planned_sources" Name="resetfp_mrp_planned_sources" Size="50,20">

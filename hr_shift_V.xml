<search next="records_hr_shift" listed="0" prefixwhere="1" prefixand="0"  inputed="1">%{searchtmpl}</search><submit next="GBDEDIThr_shift" namelist="search" function="search"/>	
</OnClick></Button><TAB/></Ctrls></GroupBox>

	<DbDetails>
<DbTable>xbs.prj_rate_schedule_header</DbTable>
<Index>prj_rate_schedule_header_id</Index>
<Data>schedule_name,rate_type,prj_rate_schedule_header_id</Data>
<Where></Where>
<queryFormat></queryFormat></DbDetails><OnSelEndOk>
<if cond="%{prj_rate_schedule_header_id[rate_type]}=='EMPLOYEE'"><DbDetails><DbTable>xbs.employees_v</DbTable><Data>employee_name as name,hr_employee_id refid</Data></DbDetails><submit next="reference_key_value" namelist="DbDetails"/>
